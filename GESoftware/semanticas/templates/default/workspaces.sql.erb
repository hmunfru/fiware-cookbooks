--
-- Estructura de tabla para la tabla `ontologyinstance`
--

CREATE TABLE IF NOT EXISTS `OntologyInstance` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `context` varchar(255) NOT NULL,
  `ontology_name` varchar(255) NOT NULL,
  `out_of_date` tinyint(1) NOT NULL,
  `version` bigint(20) NOT NULL,
  `ontology_registry_id` bigint(20) NOT NULL,
  `workspace_id` bigint(20) NOT NULL,
  PRIMARY KEY (`id`),
  KEY `FK4C8B87241D16F070` (`ontology_registry_id`),
  KEY `FK4C8B872486AA523` (`workspace_id`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;

